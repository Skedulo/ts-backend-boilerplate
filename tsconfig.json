{
  "compilerOptions": {
    "incremental": true,
    "target": "es2019",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "lib": ["es2019"],
    "declaration": false,
    "outDir": "dist/",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
